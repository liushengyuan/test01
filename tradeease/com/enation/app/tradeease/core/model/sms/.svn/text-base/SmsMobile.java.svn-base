package com.enation.app.tradeease.core.model.sms;

import java.sql.Timestamp;

import com.enation.framework.database.PrimaryKeyField;

/**
 * 短信发送实体
 * @author zmm
 *
 */
public class SmsMobile {

	private Integer id;//主键
	
	private String v_servicecode;//业务编码
	
	private String v_mobile;//客户手机号码
	
	private String v_content;//短信内容，不超过500个字符
	
	private String v_smstype;//短信类型
	
	private String v_status; //短信发送状态
	
	private Long sendtime; //短信发送状态

	@PrimaryKeyField
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getV_servicecode() {
		return v_servicecode;
	}

	public void setV_servicecode(String v_servicecode) {
		this.v_servicecode = v_servicecode;
	}

	public String getV_mobile() {
		return v_mobile;
	}

	public void setV_mobile(String v_mobile) {
		this.v_mobile = v_mobile;
	}

	public String getV_content() {
		return v_content;
	}

	public void setV_content(String v_content) {
		this.v_content = v_content;
	}

	public String getV_smstype() {
		return v_smstype;
	}

	public void setV_smstype(String v_smstype) {
		this.v_smstype = v_smstype;
	}

	public String getV_status() {
		return v_status;
	}

	public void setV_status(String v_status) {
		this.v_status = v_status;
	}

	public Long getSendtime() {
		return sendtime;
	}

	public void setSendtime(Long sendtime) {
		this.sendtime = sendtime;
	}

	
	
	
	
	
}
