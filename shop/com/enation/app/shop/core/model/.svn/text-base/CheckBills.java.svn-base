package com.enation.app.shop.core.model;

import java.io.Serializable;

public class CheckBills implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int bills_id;
	
	private String oid; 		// 订单号
	
	private Double amount;	// 订单金额
	
	private String pmode;		// 支付卡种
	
	private String pstring;		// 订单支付状态
	
	private String pdesc;		// 订单支付状态说明
	
	private String mac;			// md5 签名信息

	public int getBills_id() {
		return bills_id;
	}

	public void setBills_id(int bills_id) {
		this.bills_id = bills_id;
	}

	public String getOid() {
		return oid;
	}

	public void setOid(String oid) {
		this.oid = oid;
	}

	public Double getAmount() {
		return amount;
	}

	public void setAmount(Double amount) {
		this.amount = amount;
	}

	public String getPmode() {
		return pmode;
	}

	public void setPmode(String pmode) {
		this.pmode = pmode;
	}

	public String getPstring() {
		return pstring;
	}

	public void setPstring(String pstring) {
		this.pstring = pstring;
	}

	public String getPdesc() {
		return pdesc;
	}

	public void setPdesc(String pdesc) {
		this.pdesc = pdesc;
	}

	public String getMac() {
		return mac;
	}

	public void setMac(String mac) {
		this.mac = mac;
	}
	
}
