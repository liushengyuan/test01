*{
	padding:0;
	margin:0 auto;
  font-family: "微软雅黑", "microsoft yahei", "宋体";
 }
 body{
  background: #fcfcc6;
 }
ul {
  list-style: none; }

a {
  text-decoration: none; }
img{border:none;}
.seckill_main{
  width:100%; height:100%; min-height:100%;
}
.seckill_banner{
  width:100%;
}
.seckill_banner img{
  width: 100%;
}
.seckill_title{
  height: 45px; width: 100%; background: #ad0111 url(../images/bj_a.png) center no-repeat; color: #fff;
  font-size: 18px; text-align: center; line-height: 45px;
}
.seckill_content{
  width: 1050px; margin: 0 auto; padding-top:30px;
}
.seckill_content img{
  display: inline;
}
.conten_paddint{
  padding-top: 0;
}


















