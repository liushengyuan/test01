/* 热卖商品    */
#weekly-bestsellings .hot_news{
	background:#fff; border:1px solid #ccc; border-top:none; padding-bottom:15px;
}
.hot_div{
	width:219px;
}
.hot_div .hot_news_a{
	width:219px; padding:0; border:2px solid #F2F2F2;
}
.hot_div .hot_news_a:hover{
	border:2px solid #b50029;
}
.hot_div .hot_news_a .nnnn-img{
	width:219px; height:219px;
}
.hot_div .hot_news_a .nnnn-img img{
	width:219px; height:219px; max-width:219px; max-height:219px;
}
.hot_div .wb-link{
	 padding-bottom:10px;
}
.hot_div .wb-info .wb-title{
	width:auto;
}
.hot_div .market {
    float: left;
    line-height: 20px;
    margin-top: 4px;
    color: #999;
        font-size: 12px;
    text-decoration: line-through;
}
.hot_div .buy {
    line-height: 14px;
    height: 22px;
    padding: 3px 5px;
    border-radius: 2px;
    background: #F7F7F7;
    color: #fff;
    border:none;
    float: right;
    cursor: pointer;
    border-color: #950b00;
    color: #fff;
    margin-right: 8px;
    background-color: #950b00;
    background-repeat: repeat-x;
    background-image: -moz-linear-gradient(top,#E74649,#950b00);
    background-image: -webkit-linear-gradient(top,#E74649,#950b00);
    background-image: -o-linear-gradient(top,#E74649,#950b00);
    background-image: linear-gradient(top,#E74649,#950b00);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#E74649', endColorstr='#950b00', GradientType=0);
}

/* 分类  */
.country_title_new{
	height:50px; background:#b50029;
}
.country_title_new ul{}
.country_title_new ul li{
	float:left; width:20%; text-align:center; height:50px;
}
.country_title_new ul li a{
	display:block; float:left; height:50px; font-size:18px; color:#fff; line-height:48px; text-align:center; width:100%;
}
.country_title_new ul li a:hover{
	background:#fff url('../images/ico_b.png') 55px center no-repeat; color:#b50029;
}
.country_title_new ul li .country_link{
	background:#fff url('../images/ico_b.png') 55px center no-repeat; color:#b50029;
}
.mui-act-items .mui-act-item {
    display: inline;
    float: left;
}
.mui-act-items-240 .mui-act-item {
    margin: 0 10px 10px 0;
    width: 230px;
}
.mui-act-items .isdesc li {
    height: 360px;
}
.mui-act-items li .mui-act-item-img {
    height: 240px;
    overflow: hidden;
    position: relative;
    width: 230px;
}
.mui-act-items li .mui-act-item-img .mui-act-item-imgtextbg {
    background: #000 none repeat scroll 0 0;
    bottom: 0;
    height: 20px;
    left: 0;
    opacity: 0.45;
    position: absolute;
    width: 230px;
}
.mui-act-items li .mui-act-item-desc {
    background: #fff none repeat scroll 0 0;
    font-size: 12px;
    height: 30px;
    line-height: 15px;
    position: relative;
    text-align: center;
    width: 230px;
}
.mui-act-items li .mui-act-item-logo {
    height: 35px;
    /* left: 5px;
    top: 282px; */
    position: absolute;
    width: 70px;
}

.mui-act-items li .mui-act-item-yprice {
    color: #a1a1a1;
    font-size: 12px;
    height: 18px;
    line-height: 18px;
    overflow: hidden;
    padding-left: 84px;
    padding-right: 5px;
    padding-top: 8px;
    text-align: right;
}
.mui-act-items li .mui-act-item-yprice {
    color: #a1a1a1;
    font-size: 12px;
    line-height: 18px;
    text-align: right;
}
.mui-act-items .item-price-icon {
    font-family: arial !important;
    font-size: 12px !important;
    font-weight: 400 !important;
    margin: 0 0 0 5px;
}
.mui-act-items li .mui-act-item-xprice {
    color: #ff1b50; text-align:center;
}
.mui-act-items li .mui-act-item-xprice span {
    font-family: helvetica;
    font-size: 16px;
    font-weight: 700;
}
.mui-act-items li .mui-act-item-yhq {
    height: 30px;
    overflow: hidden;
    padding: 0 4px 4px;
    width: 230px;
}
.mui-act-items li .mui-act-item-btn {
    background: #ff1b50 none repeat scroll 0 0; color:#fff; text-align:center; margin-top:5px;
}
.mui-act-items li .mui-act-item-btn span.hur {
    margin-left: 2px;
}

