	function jiage(){
	var jgfull=$("#good_price1").html();
	var locale="${locale}";
	if(locale=="zh"){
		var subpfj=jgfull.lastIndexOf("￥");
		var pfjbefore=jgfull.substring(1,subpfj-1);
		var pfjafter=jgfull.substring(subpfj+1,jgfull.length);
		if((pfjbefore==0 && pfjafter==0)||pfjafter==0){
			$("#be_jgworth").hide();
		}
		if((pfjbefore>0 && pfjafter>0 && pfjbefore==pfjafter )){
			$("#good_price1").html("￥"+pfjbefore);
		}
	}else{
		var subpfj=jgfull.lastIndexOf("-");
		var pfjbefore=jgfull.substring(0,subpfj-2);
		var pfjafter=jgfull.substring(subpfj+1,jgfull.length-2);
		if((pfjbefore==0 && pfjafter==0)||pfjafter==0){
			$("#be_jgworth").hide();
		}
		if((pfjbefore>0 && pfjafter>0 && pfjbefore==pfjafter )){
			$("#good_price1").html(pfjbefore+"р.");
		}
	}
	}
	
