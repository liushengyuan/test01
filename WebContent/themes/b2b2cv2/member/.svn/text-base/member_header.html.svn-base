<script>
	$(function(){
        $(".side_menu h1").click(function(){
            var changeclass = $(this).find("span").attr("class");
            if(changeclass=="down") {
                $(this).next("ul").hide();
                $(this).find("span").removeClass("down").addClass("top");
            }else{
                $(this).next("ul").show();
                $(this).find("span").removeClass("top").addClass("down");
			}
        })
