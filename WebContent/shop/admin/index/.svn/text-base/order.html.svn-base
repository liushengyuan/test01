<table cellspacing="0" cellpadding="0" border="0" width="100%">
      <tbody>        
         <tr>
            <th><span>未处理订单：</span></th>
            <td><span><a <#if (orderss.wait > 0)> href="${ctx }/shop/admin/order!list.do?status=-99" class="imp" </#if> >${ orderss.wait}</a>个</span></td>
          </tr>
          <tr>
            <th><span>待发货订单：</span></th>
            <td><span><a <#if (orderss.allocation_yes > 0)> href="${ctx }/shop/admin/order!notShipOrder.do" class="imp" </#if> >${ orderss.allocation_yes}</a>个</span></td>
          </tr>
          <tr>
            <th><span>待结算订单：</span></th>
            <td><span><a <#if (orderss.not_pay > 0)> href="${ctx }/shop/admin/order!notPayOrder.do" class="imp" </#if> >${ orderss.not_pay}</a>个</span></td>
          </tr>   
          <tr>
            <th><span>已成交订单数：</span></th>
            <td><span><a <#if (orderss.complete > 0 )>  href="${ctx }/shop/admin/order!list.do?complete=yes" class="imp" </#if> >${ orderss.complete}</a>个</span></td>
          </tr>
        </tbody>            
</table>